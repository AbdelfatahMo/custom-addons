<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="registration_application_cron" model="ir.cron">
        <field name="name">Age</field>
        <field name="model_id" ref="model_registration_application"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="state">code</field>
        <field name="code">model._corn_age()</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
    </record>

</odoo>
